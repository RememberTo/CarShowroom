        <Button x:Name="ButtoMinimaized_Copy" 
            Style="{StaticResource MaterialDesignFlatDarkButton}"
            Content="xxxxxxxxxxxxx" HorizontalAlignment="Left" FontSize="20" FontWeight="Bold"  
