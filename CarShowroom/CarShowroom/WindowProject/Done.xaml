        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarShowroom.WindowProject"
        mc:Ignorable="d"
         Background="{DynamicResource PrimaryHueMidBrush}"
        Title="Выполнено" Height="252" Width="368" WindowStartupLocation="CenterScreen" ResizeMode ="NoResize">
    <Grid Margin="0,0,0,1">
        <Button Content="ОК" 
                x:Name="button_OK"
                Style="{StaticResource MaterialDesignFlatDarkButton}"
                Background="{DynamicResource PrimaryHueDarkBrush}"
                HorizontalAlignment="Left" Margin="252,167,0,0" VerticalAlignment="Top" 
                Width="92" Cursor="Hand"  Height="27" Click="button_getPrice_Click" Foreground="White" />
        <TextBlock
            x:Name="lblString"
            Text="" HorizontalAlignment="Left" Margin="64,43,0,0" VerticalAlignment="Top" Height="110" Width="268" Foreground="#DDFFFFFF"/>
    </Grid>
</Window>
